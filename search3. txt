hammer --csv --no-headers package list \
  --organization " Linux" \
  --content-view "CCV RHEL" \
  --lifecycle-environment "Nonprod" \
  --search 'name = kernel' \
  --per-page 10000 \
  --fields filename \
| awk -F, '
  function key(fn, m){
    if (fn ~ /el7/) return "RHEL7";
    if (fn ~ /el8/) return "RHEL8";
    if (match(fn, /el9_[0-9]+/, m)) return m[0];   # el9_4, el9_6
    return "";
  }
  { k=key($1); if(k!="") print k "," $1 }
' \
| sort -t, -k1,1 -k2,2V -r \
| awk -F, '!seen[$1]++ { print $2 }'